{
  "name": "node-opcua-crypto",
  "version": "1.7.1",
  "description": "Crypto tools for Node-OPCUA",
  "main": "./index.js",
  "types": "./index.d.ts",
  "browser": "./index_web.js",
  "scripts": {
    "build": "npx tsc",
    "prepare": "npm run build",
    "tslint": "tslint --project . --fix",
    "lint": "eslint . --ext .ts",
    "test": "mocha test -r ts-node/register -r source-map-support/register -R spec --recursive --timeout 200000 --bail test/**/*.ts",
    "makedoc": "npx typedoc -out doc",
    "cost-of-modules": "npx cost-of-modules --no-install",
    "release-it": "npx release-it",
    "prettier-format": "prettier --config .prettierrc.js lib/**/*.ts test/**/*.ts --write",
    "ncu": "npx npm-check-updates -u"
  },
  "keywords": [
    "OPCUA",
    "opcua",
    "m2m",
    "iot",
    "opc ua",
    "internet of things"
  ],
  "author": "Etienne Rossignon",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@types/should": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lorem-ipsum": "^2.0.3",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "should": "^13.2.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "better-assert": "^1.0.2",
    "chalk": "^4.1.0",
    "hexy": "^0.3.1",
    "jsrsasign": "^10.1.5",
    "sshpk": "^1.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-opcua/node-opcua-crypto.git"
  },
  "bugs": {
    "url": "https://github.com/node-opcua/node-opcua-crypto/issues"
  }
}
